<section id="five" class="section app__dashboard-container wrapper special">
  <header class="major">
    <h2 class="app__dashboard-headline">Bienvenido <span class="app__dashboard_username"><%= current_user.store_name  %> </span></h2>
    <p>Es hora de convertir tus imagenes en productos</p>
  </header>
  <div class="app__dashboard-gallery ">
    <p>Haz clic en la que desees convertir en producto</p>
    <% @recent_media_items.each do |media| %>
        <div class=" thumbnail app__dashboard_gallery-thumbnail col-xs-4 col-sm-3 col-md-2 col-lg-3">
          <% if Product.exists?(:photo_id => media.id) %>
            <img src='<%= media.images.thumbnail.url %>' class="app__dashboard_gallery-blur app__dashboard_gallery-img img-responsive" alt="product">
          <% else %>
            <%= link_to new_product_path(photo: media.images.standard_resolution.url, photo_id: media.id)  do %>
              <img src='<%= media.images.thumbnail.url %>' class="app__dashboard_gallery-img img-responsive" alt="product">
              <p class="app__dashboard_gallery-new">
                <i class="fa fa-shopping-bag"></i>
              </p>
            <% end %>
          <% end %>
        </div>
    <% end %>
  </div>
  <%- #if !@recent_media_items.last.id.nil? -%>
    <%- #link_to "Ver mÃ¡s" , users_dashboard_path(max_id: @recent_media_items.last.id)  -%>
    <%- # end -%>




</section>
