<!DOCTYPE html>
<html>
<head>
  <title>VirtualBookletApp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%= favicon_link_tag 'favicon.ico' %>
  <%= favicon_link_tag  'apple-icon-57x57.png' , rel: "apple-touch-icon" %>
  <%= favicon_link_tag  'apple-icon-60x60.png' , rel: "apple-touch-icon" %>
  <%= favicon_link_tag  'apple-icon-72x72.png' , rel: "apple-touch-icon" %>
  <%= favicon_link_tag  'apple-icon-76x76.png' , rel: "apple-touch-icon" %>
  <%= favicon_link_tag  'apple-icon-114x114.png' , rel: "apple-touch-icon" %>
  <%= favicon_link_tag  'apple-icon-120x120.png' , rel: "apple-touch-icon" %>
  <%= favicon_link_tag  'apple-icon-144x144.png' , rel: "apple-touch-icon" %>
  <%= favicon_link_tag  'apple-icon-152x152.png' , rel: "apple-touch-icon" %>
  <%= favicon_link_tag  'apple-icon-180x180.png' , rel: "apple-touch-icon" %>
  <meta name="viewport" content="width=device-width, initial-scale=1">
</head>
<body>

  <div class="container app__container">
    <%= render "helpers/menu" %>
    <%= yield %>
    <%= render "helpers/footer" %>
  </div>

  <%= render "mixins/search" %>

</body>
</html>
